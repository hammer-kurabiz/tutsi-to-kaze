	window.addEventListener('DOMContentLoaded', function(){
      var floatingBnr = $('#subscription-floating-bnr');
      var subscFloatingBnr = $('.subsc-floating-bnr');
      floatingBnr.hide();
      subscFloatingBnr.hide();
      $(window).on('load scroll resize', function () {
          var wid = window.innerWidth;
          if ($(this).scrollTop() > 100) {//スクロールが100に達したら
              floatingBnr.fadeIn();//ページトップのボタン表示
              subscFloatingBnr.fadeIn();//ページトップのボタン表示
          } else {
              floatingBnr.fadeOut();
              subscFloatingBnr.fadeOut();
          }
      });
     });
  </script>
  
